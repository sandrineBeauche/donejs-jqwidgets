{
  "name": "donejs-jqwidgets",
  "version": "0.0.0",
  "description": "An awesome DoneJS app",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donejs-user/essai.git"
  },
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "private": true,
  "scripts": {
    "test": "testee test.html --config=testee.json",
    "start": "done-serve --port 8080",
    "develop": "done-serve --develop --port 8080",
    "build": "node build"
  },
  "main": "donejs-jqwidgets/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "",
    "donejs-app"
  ],
  "steal": {
    "main": "donejs-jqwidgets/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": "",
    "paths": {
      "jquery": "node_modules/jquery/dist/jquery.js",
      "jqwidgets/*": "node_modules/jqwidgets-framework/jqwidgets/*.js"
    },
    "meta": {
      "jqwidgets/jqxcore": {
        "format": "global",
        "deps": [
          "jquery"
        ]
      },
      "jqwidgets/jqxbuttons": {
        "format": "global",
        "deps": [
          "jquery"
        ]
      },
      "jqwidgets/jqx-all": {
        "format": "global",
        "deps": [
          "jquery"
        ]
      }
    }
  },
  "dependencies": {
    "can-component": "^3.3.4",
    "can-connect": "^1.5.8",
    "can-define": "^1.5.4",
    "can-view-autorender": "^3.1.1",
    "done-autorender": "^1.3.1",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^1.4.0",
    "generator-donejs": "^1.0.7",
    "jquery": "^3.2.1",
    "jqwidgets-framework": "^4.6.4",
    "steal": "^1.6.0",
    "steal-less": "^1.2.0",
    "steal-stache": "^3.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "donejs-cli": "^1.0.0",
    "funcunit": "^3.2.0",
    "steal-mocha": "^1.0.0",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.8.4",
    "testee": "^0.7.0"
  },
  "license": "MIT"
}
